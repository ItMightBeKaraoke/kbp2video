	msgpack==1.1.1
	packaging==25.0
	PySide6_Essentials==6.9.1
	python-dateutil==2.9.0.post0
	PyYAML==6.0.2
	requests==2.32.5
	shiboken6==6.9.1
	six==1.17.0
	soupsieve==2.7
	tqdm==4.67.1
	typing_extensions==4.14.1
	urllib3==1.26.20
local_wheels = sgmllib3k-1.0.0-py3-none-any.whl

