	idna==3.11
	kbputils==${kbputils_version}
	lastversion==3.6.4
	msgpack==1.1.2
	packaging==25.0
	PySide6_Essentials==6.10.1
	python-dateutil==2.9.0.post0
	PyYAML==6.0.3
	requests==2.32.5
	shiboken6==6.10.1
	six==1.17.0
	soupsieve==2.8.1
	tqdm==4.67.1
	typing_extensions==4.15.0
	urllib3==2.6.2
local_wheels = sgmllib3k-1.0.0-py3-none-any.whl

