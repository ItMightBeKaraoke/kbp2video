[Application]
name=kbp2video
version=${version}
script=kbp2video.pyw

[Build]
installer_name=kbp2video_setup_${version}.exe

[Shortcut kbp2video (debug mode)]
script=kbp2video.pyw
console=true

[Python]
version=3.14.2
include_msvcrt=false
bitness=64

[Include]
pypi_wheels = kbp2video==${version}
	appdirs==1.4.4
	ass==1.0.3
	beautifulsoup4==4.14.3
	CacheControl==0.14.4
	certifi==2025.11.12
	charset-normalizer==3.4.4
	distro==1.9.0
	feedparser==6.0.12
	ffmpeg-python==0.2.0
	filelock==3.20.2
