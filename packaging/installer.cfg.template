[Application]
name=kbp2video
version=${version}
script=kbp2video.pyw

[Build]
installer_name=kbp2video_setup_${version}.exe

[Shortcut kbp2video (debug mode)]
script=kbp2video.pyw
console=true

[Python]
version=3.13.7
include_msvcrt=false
bitness=64

[Include]
pypi_wheels = kbp2video==${version}
	appdirs==1.4.4
	ass==0.5.4
	beautifulsoup4==4.13.4
	CacheControl==0.14.3
	certifi==2025.8.3
	charset-normalizer==3.4.3
	distro==1.9.0
	feedparser==6.0.11
	ffmpeg-python==0.2.0
	filelock==3.19.1
